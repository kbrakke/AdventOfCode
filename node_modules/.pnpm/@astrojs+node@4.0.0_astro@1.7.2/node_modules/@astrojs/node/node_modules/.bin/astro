#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/kbrakke/ws/AdventOfCode/AOC-2022/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/kbrakke/ws/AdventOfCode/AOC-2022/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../astro@1.7.2/node_modules/astro/astro.js" "$@"
else
  exec node  "$basedir/../../../../../../astro@1.7.2/node_modules/astro/astro.js" "$@"
fi
